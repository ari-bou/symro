# SETS AND PARAMETERS
# --------------------------------------------------------------------------------------------------
# beginregion
param p;
# endregion 


# VARIABLES
# --------------------------------------------------------------------------------------------------
# beginregion
var x1 := 500, >= 0, <= 1000;
var x2 := 500, >= 0, <= 1000;
var y := 500, >= 0, <= 1000;
# endregion 


# OBJECTIVES
# --------------------------------------------------------------------------------------------------
# beginregion
minimize OBJ: 10 * p * y + -1 * x1 + -1 * x2;
# endregion 


# CONSTRAINTS
# --------------------------------------------------------------------------------------------------
# beginregion
CON1: -p * y + x1 <= 0;
CON2: p * x2 + x1 + -1 * 15 = 0;
CON3: 0.1 + -1 * x2 <= 0;
# endregion 

