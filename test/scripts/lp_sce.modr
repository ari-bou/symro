# SETS AND PARAMETERS
# --------------------------------------------------------------------------------------------------
# beginregion
set S := {1,2,3};
param p{S};
param cut_count = 2;
param lambda_1{s in S, 1..cut_count};
param y_soln = 0.4;
# endregion 


# VARIABLES
# --------------------------------------------------------------------------------------------------
# beginregion
var x1{S} := 500, >= 0, <= 1000;
var x2{S} := 500, >= 0, <= 1000;
var y{S} := 500, >= 0, <= 1000;
# endregion 


# OBJECTIVES
# --------------------------------------------------------------------------------------------------
# beginregion
minimize OBJ: (sum {s in S} (x1[s] + x2[s] + -1 * 10 * p[s] * y[s]));
minimize OBJ_SUB{s in S}: (x1[s] + x2[s] + -1 * 10 * p[s] * y[s]);
minimize OBJ_SUB_PrimalSubproblem{s in S}: (x1[s] + x2[s] + -1 * 10 * p[s] * y[s]);
# endregion 


# CONSTRAINTS
# --------------------------------------------------------------------------------------------------
# beginregion
CON1{s in S}: p[s] * y[s] + -1 * x1[s] <= 0;
CON2{s in S}: p[s] * x2[s] + x1[s] + -1 * 15 = 0;
CON3{s in S}: 0.1 + -1 * x2[s] <= 0;
NON_ANT{s in S}: y[s] + -1 * y[1] = 0;
# endregion 

